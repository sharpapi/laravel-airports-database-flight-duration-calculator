{
    "name": "sharpapi/laravel-airports-database-flight-duration-calculator",
    "version": "1.0.2",
    "description": "Airports Database & Flight Duration Calculator for Laravel powered by SharpAPI.com",
    "keywords": [
        "sharpapi",
        "ai-powered",
        "api",
        "ai api",
        "api integration",
        "restful api",
        "php",
        "laravel",
        "software development",
        "airports",
        "flight duration",
        "travel",
        "aviation",
        "iata",
        "icao"
    ],
    "homepage": "https://github.com/sharpapi/laravel-airports-database-flight-duration-calculator",
    "license": "MIT",
    "authors": [
        {
            "name": "Dawid Makowski",
            "email": "contact@sharpapi.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "laravel/framework": "^10.48.29|^11.0|^12.0",
        "sharpapi/php-core": "^1.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "SharpAPI\\AirportsDatabaseFlightDurationCalculator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SharpAPI\\AirportsDatabaseFlightDurationCalculator\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "",
        "build": [],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SharpAPI\\AirportsDatabaseFlightDurationCalculator\\AirportsDatabaseFlightDurationCalculatorProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
